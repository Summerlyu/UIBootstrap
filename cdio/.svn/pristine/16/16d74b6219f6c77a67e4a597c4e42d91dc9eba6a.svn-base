package cn.edu.fjnu.cdio.model.pym;

import java.util.Date;


public class Sc {

	private ScId id;   //主键
	private String type;  //1表示未购买，0表示已购买
	private Date time;    //购买时间
	
	public Sc() {
	}

	public Sc(ScId id, String type) {
		this.id = id;
		this.type = type;
	}

	public ScId getId() {
		return id;
	}

	public void setId(ScId id) {
		this.id = id;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Date getTime() {
		return time;
	}

	public void setTime(Date time) {
		this.time = time;
	}
	
}
