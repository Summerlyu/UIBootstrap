    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    clear: both;
    color: #3F463F;
    height: 30px;
    line-height: 30px;
    margin: 0;
    text-align: center;
    text-shadow: 0 2px 2px #87C4B2;
}
.sous-menu ul {
    display: inline;
    text-align: center;
}
.sous-menu li {
    display: inline;
    list-style: none outside none;
    margin: 0 2px;
}
.sous-menu a {
    color: #3F463F;
    font-size: 14px;
    font-weight: bold;
    letter-spacing: 1px;
    text-decoration: none;
    text-shadow: 0 2px 2px #87C4B2;
}
