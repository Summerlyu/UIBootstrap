}
.menu {
    background: url("/cdio2010/image/coa/menu-top.png") no-repeat scroll center top #76AC9C;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    clear: both;
    color: #3F463F;
    height: 60px;
    line-height: 60px;
    margin: 5px 0 0;
    text-align: center;
    text-shadow: 0 2px 2px #87C4B2;
}
.menu ul {
    display: inline;
    text-align: center;
}
.menu li {
    display: inline;
    list-style: none outside none;
    margin: 0 7px;
}
.menu a {
    color: #3F463F;
    font-size: 22px;
    font-weight: bold;
    letter-spacing: 1px;
    text-decoration: none;
    text-shadow: 0 2px 2px #87C4B2;
}
.menu a:hover {
    color: #FFFFFF;
}
.menu a span {
    color: #FFFFFF;
}
.menu a span:hover:after {
    content: " »";
}
.menu h3 {
    font-size: 20px;
}
.menu h3 span {
    background: none repeat scroll 0 0 #FFFFFF;
    color: #EC0000;
    margin-left: 5px;
    padding: 5px 10px;
}
.sous-menu {
