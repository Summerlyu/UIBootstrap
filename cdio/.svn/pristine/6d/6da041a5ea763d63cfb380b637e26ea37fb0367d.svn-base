@CHARSET "UTF-8";

* {
    border: 0 none;
    margin: 0;
    padding: 0;
}
body {
    background: url("/cdio2010/image/coa/bg.jpg") repeat scroll 0 0 #DBF1B6;
    color: #0A0A0A;
    cursor: default;
    font-family: 'Quattrocento Sans',serif;
    margin: auto;
    min-width: 1200px;
    text-shadow: 0 1px 1px #FFFFFF;
}
body:before {
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.8);
    content: "";
    height: 10px;
    left: 0;
    position: fixed;
    top: -10px;
    width: 100%;
    z-index: 100;
}
a {
    outline: medium none;
}
h1, h2, h3, h4, h5, h6 {
    font-family: 'Oswald',sans-serif;
}
img {
    border: medium none;
    margin: 0;
    padding: 0;
}
#top {
    background: none repeat scroll 0 0 #76AC9C;
    border-bottom: 1px solid #FFFFFF;
    height: 15px;
    left: 0;
    top: 0;
    width: 100%;
}
#ecran {
    background: url("/cdio2010/image/coa/regle.png") repeat-y scroll right center transparent;
    margin: auto;
}
#page {
    margin: auto;
    padding: 12px;
    width: 1000px;
}
#header {
    background: url("/cdio2010/image/coa/header.jpg") repeat scroll 0 0 transparent;
    height: 244px;
    margin: 10px 0;
}
#header_deco {
    animation-duration: 3s;
    animation-name: header_opacity;
    margin-left: 380px;
    margin-top: 30px;
    position: absolute;
}
.noise {
