package cn.edu.fjnu.cdio.model.grp;

import java.io.Serializable;

import cn.edu.fjnu.cdio.model.common.User;

/**
 * 
 * @author lxz
 *
 *辅助类，用来存放数据
 */
@SuppressWarnings("serial")
public class GroupHelp implements Serializable{
   private int flag;  //标记是否加入该组
   private User user;
   private Topic topic;
	private Group group;

	public int getFlag() {
		return flag;
	}

	public void setFlag(int flag) {
		this.flag = flag;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public Topic getTopic() {
		return topic;
	}

	public void setTopic(Topic topic) {
		this.topic = topic;
	}

	public Group getGroup() {
		return group;
	}

	public void setGroup(Group group) {
		this.group = group;
	}
}
