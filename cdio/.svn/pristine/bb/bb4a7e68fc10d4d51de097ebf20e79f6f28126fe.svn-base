package cn.edu.fjnu.cdio.model.pym;

import java.util.Date;

import cn.edu.fjnu.cdio.model.common.User;

/**
 * 学生实体在本系统中用的申请表
 * 
 * @author 汪清姻
 * 
 */

public class ApplyTable {

	private long applyID; //主键id
	
	private String trueName; //真实姓名
	
	private String address;  //家庭地址
	
	private String email;    //电子邮箱
	
	private String phone;    //电话号码
	
	private String idCard; //身份证
	
	private Double applyEP;  //申请点数
	
	private String application; //申请书
	
	private int attestation; //申请状态
	
	private Date time;    //当前时间
	
	private String reason; //失败原因
	
	private Double reEp;     //获取的ep
	
	private byte[] pic;   //个人照片
	
	private byte[] stuCard; //学生证明
	
	private User user;    //当前用户
	
	private ReceiveGrant receive;
	
	private String evidenceName; //旁证姓名
	
	private String job;//工作职位
	
	private String workUnit; //工作单位
	
	private String evidenceEmail; //旁证人邮箱
	
	private String evidencePhone; //旁证人电话号码
	
	private String evidenceIdcard; //旁证人身份证
	
	private byte[] evidencePic; //证明材料


	public long getApplyID() {
		return applyID;
	}

	public void setApplyID(long applyID) {
		this.applyID = applyID;
	}

	public String getTrueName() {
		return trueName;
	}

	public void setTrueName(String trueName) {
		this.trueName = trueName;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getIdCard() {
		return idCard;
	}

	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}
	
	public Double getApplyEP() {
		return applyEP;
	}

	public void setApplyEP(Double applyEP) {
		this.applyEP = applyEP;
	}

	public void setReEp(Double reEp) {
		this.reEp = reEp;
	}

	public String getApplication() {
		return application;
	}

	public void setApplication(String application) {
		this.application = application;
	}

	public int getAttestation() {
		return attestation;
	}

	public void setAttestation(int attestation) {
		this.attestation = attestation;
	}

	public Date getTime() {
		return time;
	}

	public void setTime(Date time) {
		this.time = time;
	}

	public String getReason() {
		return reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public byte[] getPic() {
		return pic;
	}

	public void setPic(byte[] pic) {
		this.pic = pic;
	}

	public byte[] getStuCard() {
		return stuCard;
	}

	public void setStuCard(byte[] stuCard) {
		this.stuCard = stuCard;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public ReceiveGrant getReceive() {
		return receive;
	}

	public void setReceive(ReceiveGrant receive) {
		this.receive = receive;
	}

	public Double getReEp() {
		return reEp;
	}

	public String getEvidenceName() {
		return evidenceName;
	}

	public void setEvidenceName(String evidenceName) {
		this.evidenceName = evidenceName;
	}

	public String getJob() {
		return job;
	}

	public void setJob(String job) {
		this.job = job;
	}

	public String getWorkUnit() {
		return workUnit;
	}

	public void setWorkUnit(String workUnit) {
		this.workUnit = workUnit;
	}

	public String getEvidenceEmail() {
		return evidenceEmail;
	}

	public void setEvidenceEmail(String evidenceEmail) {
		this.evidenceEmail = evidenceEmail;
	}

	public String getEvidencePhone() {
		return evidencePhone;
	}

	public void setEvidencePhone(String evidencePhone) {
		this.evidencePhone = evidencePhone;
	}

	public String getEvidenceIdcard() {
		return evidenceIdcard;
	}

	public void setEvidenceIdcard(String evidenceIdcard) {
		this.evidenceIdcard = evidenceIdcard;
	}

	public byte[] getEvidencePic() {
		return evidencePic;
	}

	public void setEvidencePic(byte[] evidencePic) {
		this.evidencePic = evidencePic;
	}
	
}
