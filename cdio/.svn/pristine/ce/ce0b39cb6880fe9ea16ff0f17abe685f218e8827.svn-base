}

.fm-item .fm-explain {
    font-size: 12px;
    color: rgb(178, 178, 178);
    padding: 5px 0px 0px;
}

.ui-expand {
    margin: 0px 0px 17px;
    padding: 20px 0px 0px;
    background: none repeat scroll 0% 0% rgb(250, 250, 250);
    border: 1px solid rgb(239, 239, 239);
}

div.side-container {
    position: relative;
}

#certifyForm .fm-side {
    top: -100px;
}

.fm-side {
    width: 288px;
    position: absolute;
    right: 15px;
    top: 0px;
    z-index: 10;
}

body .fn-hide, body a.fn-hide {
    display: none;
}

.fm-side-preview {
    margin-top: 5px;
    position: relative;
}

#flash-box1 {
    position: absolute;
    top: 108px;
    left: 120px;
}

body .fn-hide, body a.fn-hide {
    display: none;
}

fieldset, img, a img {
    border: 0px none;
}

body {
    font: 12px Tahoma,Helvetica,Arial,Simsun;
    color: rgb(77, 77, 77);
}

}
.fm-item .i-text {
    font-size: 14px;
    color: rgb(0, 0, 0);
}

}

.fm-item .i-text {
    vertical-align: middle;
}

.i-text {
    border: 1px solid rgb(156, 156, 156);
    font-family: Tahoma;
}
 

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {
    margin: 0px;
    padding: 0px;
}

