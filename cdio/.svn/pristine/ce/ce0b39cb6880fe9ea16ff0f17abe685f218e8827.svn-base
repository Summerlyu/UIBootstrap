}

.shop-info {
    border-bottom: 0px none;
}

.verify-box .verify-status s {
    background: url("http://img01.taobaocdn.com/tps/i1/T1nj5lXfXyXXXXXXXX-148-318.jpg") no-repeat scroll 0 -210px transparent;
    display: block;
    float: left;
