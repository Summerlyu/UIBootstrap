	 background: none repeat scroll 0% 0% rgb(247, 247, 247);
	 width:100%;

}

.verify-line.fm-line {
	float: left;
    width: 26%;
    margin-left: -35%;
    text-align: right;
    font-size: 18px;
    padding-top: 1%;
}

.verify-module .hd {
    height: 42px;
    line-height: 42px;
}

.clearfix {
}

.verify-module .hd .title {
    font-size: 24px;

    font-family: '微软雅黑';
}

.verify-disable .hd .title {
    color: rgb(153, 153, 153);
}

.verify-module .hd {
    line-height: 42px;
}

.verify-disable .hd .title .num {
    background-position: -135px 0px;
}

.verify-module .hd .title i {
    background: url("http://img01.taobaocdn.com/tps/i1/T1x9zMXolgXXcth2.f-206-102.png") no-repeat scroll -9999px -9999px transparent;
    font-size: 20px;
    font-style: normal;
    text-align: center;
    color: rgb(255, 255, 255);
    line-height: 25px;
    width: 26px;
    height: 26px;
    display: inline-block;
    margin-right: 6px;
}

.verify-module .bd {
    background: none repeat scroll 0% 0% rgb(255, 255, 255);
}

.verify-disable .bd {
    border: 1px solid rgb(230, 230, 230);
}

.verify-module .bd .shop-verify-wrap {
    padding: 25px 0px;
}

.verify-module .bd .shop-result {
    padding: 0px 40px;
}

