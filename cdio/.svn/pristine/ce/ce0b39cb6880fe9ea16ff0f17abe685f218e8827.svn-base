input, textarea, select {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
}

div.fm-selectbox .i-radio {
    vertical-align: top;
}

.fm-selectbox .i-radio {
    margin-top: 5px;
}

label#newLab {
    width: 148px;
}

label#newLab, label#oldLab, label#temperLab {
    padding-top: 50px;
    color: rgb(178, 178, 178);
    display: inline-block;
    height: 10px;
    width: 70px;
    background: url("https://img.alipay.com/pa/page/certify/cardLogo.png") no-repeat scroll 0% 0% transparent;
}

label#temperLab {
    background-position: -218px 0px;
}

.fm-label .required {
    margin-right: 6px;
