#cdman3 \u65e5\u5fd7\u8f93\u51fa\u63a7\u5236\u6587\u4ef6
log4j.rootLogger=info,sb3-stdout,sb3-file
log4j.logger.org.hibernate=debug
log4j.logger.org.springframework=debug

#stdout appender
log4j.appender.sb3-stdout=org.apache.log4j.ConsoleAppender
log4j.appender.sb3-stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.sb3-stdout.layout.ConversionPattern= %d{yyyyMMdd HH:mm:ss,SSS} %5p [%c] %m%n

#cdman3
log4j.appender.sb3-file=org.apache.log4j.DailyRollingFileAppender
log4j.appender.sb3-file.File=../logs/hotelsys.log
log4j.appender.sb3-file.layout=org.apache.log4j.PatternLayout
log4j.appender.sb3-file.layout.ConversionPattern=%-5p %d{yyyy-MM-dd HH:mm:ss,SSS} [%C:%M] - %m%n


 