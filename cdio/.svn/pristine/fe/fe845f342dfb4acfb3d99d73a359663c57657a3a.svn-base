@CHARSET "UTF-8";
.table th, .table td {
    text-align: center;
}
.page-skip {
	color:#005580;
    display: inline-block;
    font-family: Tahoma,SimSun,Arial;
    height: 24px;
    line-height: 24px;
    margin: 0;
    min-width: 16px;
    padding: 0 5px;
    text-align: center;
    vertical-align: top;
    white-space: nowrap;
}
.pagination .input-append{
	margin-top: -12px;
}
.pagination .span1{
	height: 30px;
}