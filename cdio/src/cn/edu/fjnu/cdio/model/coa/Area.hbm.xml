<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="cn.edu.fjnu.cdio.model.coa.Area" table="t_area">
  <id name="areaid" type="java.lang.Integer">
   <column name="areaid"/>
   <generator class="increment" />
  </id>
 <property name="aid" type="java.lang.String">
   <column name="aid" length="30" />
 </property>
  <property name="aname" type="java.lang.String">
   <column name="aname" length="30" />
 </property>  
  <property name="cid" type="java.lang.String">
   <column name="cid" length="30" />
 </property>
 </class>
</hibernate-mapping>
