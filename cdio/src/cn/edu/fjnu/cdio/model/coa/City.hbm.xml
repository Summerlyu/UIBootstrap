<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="cn.edu.fjnu.cdio.model.coa.City" table="t_city">
  <id name="cityid" type="java.lang.Integer">
   <column name="cityid"/>
   <generator class="increment" />
  </id>
  <property name="cid" type="java.lang.String">
   <column name="cid" length="30" />
    </property>
     <property name="cname" type="java.lang.String">
   <column name="cname" length="30" />
 </property> 
 <property name="pid" type="java.lang.String">
   <column name="pid" length="30" />
 </property>
 
 </class>
</hibernate-mapping>
